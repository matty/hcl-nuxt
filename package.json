{
    "name": "@tfbuilder/hcl-nuxt",
    "version": "0.1.0",
    "description": "Nuxt v4 module for JSON to HCL (HashiCorp Configuration Language) conversion",
    "license": "MIT",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/types.d.mts",
            "import": "./dist/module.mjs"
        }
    },
    "main": "./dist/module.mjs",
    "typesVersions": {
        "*": {
            ".": [
                "./dist/types.d.mts"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "prepack": "nuxt-module-build build",
        "dev": "nuxi dev examples",
        "dev:build": "nuxi build examples",
        "dev:prepare": "nuxt-module-build build --stub && nuxi prepare examples",
        "build": "nuxt-module-build build",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@nuxt/kit": "^3.17.5"
    },
    "devDependencies": {
        "@nuxt/module-builder": "^1.0.2",
        "nuxt": "^4.2.1",
        "vitest": "^3.2.4"
    },
    "peerDependencies": {
        "nuxt": "^3.0.0 || ^4.0.0"
    }
}